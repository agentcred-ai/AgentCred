{
  "name": "@agentcred-ai/api",
  "version": "0.1.3",
  "description": "AgentCred API - Cloudflare Worker for credential verification",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.8",
    "@cloudflare/workers-types": "^4.20260131.0",
    "tsup": "catalog:default",
    "typescript": "catalog:default",
    "vitest": "catalog:default",
    "wrangler": "^4.61.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentcred-ai/agentcred",
    "directory": "packages/api"
  },
  "homepage": "https://agentcred.dev",
  "bugs": {
    "url": "https://github.com/agentcred-ai/agentcred/issues"
  },
  "keywords": [
    "agentcred",
    "ai-agent",
    "mcp",
    "cryptography",
    "ed25519",
    "identity",
    "signature"
  ],
  "author": "AgentCred Contributors",
  "dependencies": {
    "hono": "^4.11.7",
    "jose": "^6.1.3",
    "zod": "^4.3.6"
  }
}
